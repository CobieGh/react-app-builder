{
  "name": "react-app-builder",
  "version": "0.0.2",
  "description": "A tool to build React web apps.",
  "main": "./public/app.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "prepublish": "npm run build",
    "deploy": "gh-pages-deploy",
    "clean": "rm -rf README.md LICENSE src public package.json webpack.config.js"
  },
  "author": "Kiko Ruiz Lloret",
  "license": "ISC",
  "dependencies": {
    "material-ui": "^0.15.0-beta.2",
    "normalize.css": "^4.1.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint-plugin-react": "^5.0.1",
    "eslint": "^2.8.0",
    "gh-pages-deploy": "^0.4.2",
    "node-sass": "^3.6.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  },
  "gh-pages-deploy": {
    "staticpath": "public",
    "prep": [
      "build"
    ],
    "post": [
      "clean"
    ]
  }
}
